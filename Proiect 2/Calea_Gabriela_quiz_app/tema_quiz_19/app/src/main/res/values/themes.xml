<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Tema_quiz_19" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/dark_pink</item>
        <item name="colorPrimaryVariant">@color/light_pink</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:windowBackground">@color/light_pink</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:textColorPrimary">@color/dark_pink</item>
        <item name="android:textColorSecondary">@color/black</item>
    </style>

    <style name="Theme.Tema_quiz_19.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/light_pink</item>
    </style>

    <style name="IconStyle">
        <item name="android:layout_width">64dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:tint">@color/dark_pink</item>
    </style>
    <style name="Theme.Tema_quiz_19.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="Theme.Tema_quiz_19.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
